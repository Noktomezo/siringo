{
	"name": "siringo-discord-bot",
	"description": "Multifunctional discord bot with automoderation, reaction roles, multilanguage support and more...",
	"packageManager": "yarn@4.0.1",
	"type": "module",
	"scripts": {
		"dev": "cross-env NODE_ENV=development tsx watch --no-cache src/index.ts",
		"bootstrap": "yarn build && yarn start",
		"update": "yarn upgrade-interactive",
		"start": "node dist/index.js",
		"build": "rimraf dist && tsc"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.0",
		"@commitlint/config-conventional": "^18.4.0",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/libsodium-wrappers": "^0.7.13",
		"@types/lodash-es": "^4.17.11",
		"@types/node": "^20.9.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.53.0",
		"eslint-config-neon": "^0.1.57",
		"husky": "^8.0.3",
		"lint-staged": "^15.0.2",
		"prettier": "^3.0.3",
		"rimraf": "^5.0.5",
		"tsx": "^4.1.0",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.16.0",
		"@keyv/mongo": "^2.2.8",
		"bufferutil": "^4.0.8",
		"chalk": "^5.3.0",
		"discord-hybrid-sharding": "^2.1.4",
		"discord.js": "^14.13.0",
		"distube": "^4.1.1",
		"dotenv": "^16.3.1",
		"keyv": "^4.5.4",
		"libsodium-wrappers": "^0.7.13",
		"lodash-es": "^4.17.21",
		"tiny-typed-emitter": "^2.1.0",
		"utf-8-validate": "^6.0.3",
		"ytdl-core": "^4.11.5",
		"zod": "^3.22.4"
	}
}
